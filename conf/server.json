{
    "port": "8080",
    "host":"localhost",
    "http2":{
        "enable":false,
        "key":"{server}/conf/http2/test.org.key",
        "cert":"{server}/conf/http2/test.org.crt"
    },
    "modules": [
        "lib/base.js",
        "lib/comet.js",
        "lib/mock.js",
        "lib/localdb.js"
    ],
    "ipc": {
        "socketPath": "/conf/sock/ipc.sock",
        "port": 7100,
        "host": "localhost"
    },
    "log": {
        "path": "{server}/log/",
        "level": [
            "info",
            "error"
        ],
        "maxsize": 2097152
    },
    "services":[
        {"name":"session","path":"{server}/lib/service/sessionservice.js"}
    ]
}